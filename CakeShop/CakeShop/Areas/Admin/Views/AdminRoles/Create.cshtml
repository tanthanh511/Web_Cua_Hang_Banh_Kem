@model CakeShop.Models.Role

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<div class="col-md-10 ">
    <div class="text-center">
        <div class="d-inline-flex align-items-center text-black">
            <h4 class="m-0"><a class="text-success" asp-area="Admin" asp-controller="AdminRoles" asp-action="Index">Roles</a></h4>
            <i class="fa fa-circle px-3"></i>
            <h4 class="m-0">Create</h4>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-12">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="RoleName" class="control-label"></label>
                    <input asp-for="RoleName" class="form-control" />
                    <span asp-validation-for="RoleName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Description" class="control-label"></label>
                    <input asp-for="Description" class="form-control" />
                    <span asp-validation-for="Description" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                    <a class="btn btn-info btn-tone" asp-action="Index">Back to List</a>
                </div>
            </form>
        </div>
    </div>

    <div>
       
    </div>
</div>
