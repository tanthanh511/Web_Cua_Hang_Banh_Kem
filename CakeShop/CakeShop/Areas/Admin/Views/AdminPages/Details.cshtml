        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Ordering)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ordering)
        </dd>
    </dl>
    <hr />
    <div>
        <a class="btn btn-warning btn-tone m-r-5" asp-action="Edit" asp-route-id="@Model.PageId">Edit</a> |
        <a class="btn btn-info btn-tone" asp-action="Index">Back to List</a>
    </div>
</div>

